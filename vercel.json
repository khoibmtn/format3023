{
    "version": 2,
    "builds": [
        {
            "src": "api/process.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/process",
            "dest": "/api/process.js",
            "methods": [
                "POST",
                "OPTIONS"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}